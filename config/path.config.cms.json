{
	"public": "./deploy/public",
	"browserSync": {
		"proxy": "http://local.baku2025.com"
	},

	"symbols": {
		"fileDest": "../deploy/craft/templates/includes"
	},

	"tags": {
		"src": "./deploy/craft/templates/wrapper/_layout.twig",
		"dest": "./deploy/craft/templates/wrapper/",
		"css": "/dist/css/",
		"js": "/dist/js/"
	},

	"critical": {
		"templates": "./deploy/fractal/06-pages",
		"urlBase": "./deploy/public/",
		"outputBase": "./deploy/fractal/06-pages",
		"paths": [
			{
				"input": {
					"url": "http://local.baku2025.com/",
					"source": "home/home.twig"
				},
				"output": {
					"dist": "./home"
				}
			},
			{
				"input": {
					"url": "http://local.baku2025.com/baku-and-azerbaijan",
					"source": "standard/standard.twig"
				},
				"output": {
					"dist": "./standard"
				}
			},
			{
				"input": {
					"url": "http://local.baku2025.com/timeline",
					"source": "timeline/timeline.twig"
				},
				"output": {
					"dist": "./timeline"
				}
			}
		]
	}
}
